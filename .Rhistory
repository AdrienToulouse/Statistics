Nattempts = 1e3
nsample = 100
theta = 3
samples <- lapply(1:Nattempts, function(i) rpois(nsample, theta))
realisations <- sapply(samples, function(x) estim(x, theta))
hist(realisations, probability = TRUE)
d = density(realisations, kernel='gaussian')
lines(d, col = 'red')
qqnorm(realisations)
qqline(realisations)
